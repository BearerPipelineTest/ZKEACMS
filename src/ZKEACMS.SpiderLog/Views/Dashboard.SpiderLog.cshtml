@using ZKEACMS.SpiderLog.Models
@model IEnumerable<SearchEngineVisitLog>

<div class="panel panel-default">
    <div class="panel-heading">@L("Search Engine / Spiders")</div>
    <table class="panel-body table table-bordered table-hover">
        @foreach (var item in Model)
        {
            <tr>
                <td width="80">
                    <a href="@Url.Action("Read","SpiderLog",new { Id = item.Name })" target="_blank">@L("Log")</a>
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.LastVisitAt.ToString("yyyy/MM/dd HH:mm:ss")
                </td>
            </tr>
        }
    </table>
</div>

